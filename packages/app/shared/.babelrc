{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "8"
        }
      }
    ],
    "@babel/preset-typescript"
  ],
  "ignore": [
    "**/*.d.ts"
  ],
  "sourceMaps": "both",
  "inputSourceMap": true,
  "plugins": [
    "@babel/proposal-class-properties",
    "@babel/plugin-transform-runtime"
  ]
}
